{
    "version": 2,
    "functions": {
        "api/app.py": {}
    },
    "routes": [
        {
            "src": "/api/route(.*)",
            "dest": "api/app.py?$1",
            "methods": [
                "GET",
                "POST"
            ]
        },
        {
            "src": "/api/fuel(.*)",
            "dest": "api/app.py?$1",
            "methods": [
                "GET"
            ]
        },
        {
            "src": "/api/export(.*)",
            "dest": "api/app.py?$1",
            "methods": [
                "GET"
            ]
        },
        {
            "src": "/(.*)",
            "dest": "/$1",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}