{
    "version": 2,
    "functions": {
        "api/app.py": {}
    },
    "routes": [
        {
            "src": "/",
            "dest": "api/app.py"
        },
        {
            "src": "/get_location/(.*)/(.*)",
            "dest": "api/app.py?$1&$2"
        },
        {
            "src": "/login_page",
            "dest": "api/app.py"
        },
        {
            "src": "/login",
            "dest": "api/app.py",
            "methods": [
                "GET",
                "POST"
            ]
        },
        {
            "src": "/login_success",
            "dest": "api/app.py"
        },
        {
            "src": "/route_map",
            "dest": "api/app.py"
        },
        {
            "src": "/fuel_saving",
            "dest": "api/app.py",
            "methods": [
                "GET"
            ]
        },
        {
            "src": "/export_pdf",
            "dest": "api/app.py",
            "methods": [
                "GET"
            ]
        },
        {
            "src": "/(.*)",
            "dest": "api/app.py?$1"
        }
    ]
}