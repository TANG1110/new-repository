{
    "version": 2,
    "builds": [
        {
            "src": "api/app.py",
            "use": "@vercel/python@4.0.0"
        },
        {
            "src": "static/**/*",
            "use": "@vercel/static@1.0.1"
        },
        {
            "src": "templates/**/*",
            "use": "@vercel/static@1.0.1"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "static/$1"
        },
        {
            "src": "/templates/(.*)",
            "dest": "templates/$1"
        },
        {
            "src": "/(.*)",
            "dest": "api/app.py"
        }
    ],
    "env": {
        "PYTHONUNBUFFERED": "1",
        "FLASK_APP": "api/app.py"
    }
}