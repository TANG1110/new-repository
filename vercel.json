{
    "version": 2,
    "functions": {
        "api/app.py": {}
    },
    "rewrites": [
        {
            "source": "/",
            "destination": "/api/app.py"
        },
        {
            "source": "/get_location/(.*)/(.*)",
            "destination": "/api/app.py"
        },
        {
            "source": "/login_page",
            "destination": "/api/app.py"
        },
        {
            "source": "/login",
            "destination": "/api/app.py"
        },
        {
            "source": "/login_success",
            "destination": "/api/app.py"
        },
        {
            "source": "/route_map",
            "destination": "/api/app.py"
        },
        {
            "source": "/fuel_saving",
            "destination": "/api/app.py"
        },
        {
            "source": "/export_pdf",
            "destination": "/api/app.py"
        },
        {
            "source": "/(.*)",
            "destination": "/api/app.py"
        }
    ]
}